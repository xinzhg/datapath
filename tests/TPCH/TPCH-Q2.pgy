LOAD part;
LOAD supplier;
LOAD partsupp;
LOAD nation;
LOAD region;

selpart =
FILTER
    part
BY
    MATCH(part.p_type, ".*BRASS")
    && part.p_size == 15;

selregion =
FILTER
    region
BY
    region.r_name == 'EUROPE';

/* DEBUG
PRINT
    selregion
USING
    region.r_name
AS
    r_name : HString
INTO
    "selregion.csv";
*/

j1 =
JOIN
    nation BY nation.n_regionkey,
    selregion BY region.r_regionkey;

/* DEBUG
PRINT
    j1
USING
    nation.n_nationkey,
    nation.n_name,
    region.r_name
AS
    n_nationkey : INT,
    n_name : HString,
    r_name : HString
INTO
    "j1.csv";
*/

j2 =
JOIN
    supplier BY supplier.s_nationkey,
    j1 BY nation.n_nationkey;

j3 =
JOIN
    partsupp BY partsupp.ps_partkey,
    selpart BY part.p_partkey;

/* DEBUG
PRINT
    j3
USING
    partsupp.ps_partkey
AS
    ps_partkey : INT
INTO
    "j3.csv";
*/

j4 =
JOIN
    j3 BY partsupp.ps_suppkey,
    j2 BY supplier.s_suppkey;

/* DEBUG
PRINT
    j4
USING
    part.p_partkey,
    partsupp.ps_supplycost,
    supplier.s_acctbal,
    supplier.s_name,
    nation.n_name,
    part.p_mfgr,
    supplier.s_address,
    supplier.s_phone,
    supplier.s_comment
AS
    p_partkey : INT,
    ps_supplycost : FLOAT,
    s_acctbal : FLOAT   ,
    s_name    : HString ,
    n_name    : HString ,
    p_partkey : INT     ,
    p_mfgr    : HString ,
    s_address : HString ,
    s_phone   : HString ,
    s_comment : HString
INTO
    "j4.csv";
*/

gby =
GLA:GroupBy
<
    [
        p_partkey : INT
    ],
    GLA:ExtremeTuples
    <
        [
            ps_supplycost : FLOAT : MIN
        ],
        [
            s_acctbal : FLOAT   ,
            n_name    : HString ,
            s_name    : HString ,
            p_mfgr    : HString ,
            s_address : HString ,
            s_phone   : HString ,
            s_comment : HString
        ]
    >
>
FROM
    j4
USING
    part.p_partkey,
    partsupp.ps_supplycost,
    supplier.s_acctbal,
    supplier.s_name,
    nation.n_name,
    part.p_mfgr,
    supplier.s_address,
    supplier.s_phone,
    supplier.s_comment
AS
    gby_p_partkey     : INT      ,
    gby_ps_supplycost : FLOAT    ,
    gby_s_acctbal     : FLOAT    ,
    gby_s_name        : HString  ,
    gby_n_name        : HString  ,
    gby_p_mfgr        : HString  ,
    gby_s_address     : HString  ,
    gby_s_phone       : HString  ,
    gby_s_comment     : HString;

/* DEBUG
PRINT
    gby
USING
    gby_s_acctbal,
    gby_s_name,
    gby_n_name,
    gby_p_partkey,
    gby_p_mfgr,
    gby_s_address,
    gby_s_phone,
    gby_s_comment,
    gby_ps_supplycost
AS
    s_acctbal     : FLOAT   ,
    s_name        : HString ,
    n_name        : HString ,
    p_partkey     : INT     ,
    p_mfgr        : HString ,
    s_address     : HString ,
    s_phone       : HString ,
    s_comment     : HString ,
    ps_supplycost : FLOAT
INTO
    "gby.csv";
*/

orderby =
GLA:OrderBy
<
    [
        s_acctbal : FLOAT   : DESC ,
        n_name    : HString : ASC  ,
        s_name    : HString : ASC  ,
        p_partkey : INT     : ASC
    ],
    [
        p_mfgr    : HString ,
        s_address : HString ,
        s_phone   : HString ,
        s_comment : HString
    ]
> (10)
FROM
    gby
USING
    gby_s_acctbal ,
    gby_s_name    ,
    gby_n_name    ,
    gby_p_partkey ,
    gby_p_mfgr    ,
    gby_s_address ,
    gby_s_phone   ,
    gby_s_comment
AS
    ord_s_acctbal : FLOAT    ,
    ord_s_name    : HString  ,
    ord_n_name    : HString  ,
    ord_p_partkey : INT      ,
    ord_p_mfgr    : HString  ,
    ord_s_address : HString  ,
    ord_s_phone   : HString  ,
    ord_s_comment : HString;

PRINT
    orderby
USING
    ord_s_acctbal ,
    ord_s_name    ,
    ord_n_name    ,
    ord_p_partkey ,
    ord_p_mfgr    ,
    ord_s_address ,
    ord_s_phone   ,
    ord_s_comment
AS
    s_acctbal : FLOAT   ,
    s_name    : HString ,
    n_name    : HString ,
    p_partkey : INT     ,
    p_mfgr    : HString ,
    s_address : HString ,
    s_phone   : HString ,
    s_comment : HString
INTO
    "TPCH-Q2-result.csv";
