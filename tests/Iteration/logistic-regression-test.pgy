LOAD lineitem;

agg =
GLA:LogisticRegressionIRLS (4)
FROM
    lineitem
USING
    UDF:MakeVector<4, DOUBLE>(lineitem.l_quantity, lineitem.l_extendedprice, lineitem.l_discount, lineitem.l_tax) -> VECTOR,
    1.0 // dummy scale factor
AS
    tupleCount : BIGINT;

PRINT
    agg
USING
    tupleCount
AS
    tupleCount : BIGINT
INTO
    "logistic-regression-test.csv";
